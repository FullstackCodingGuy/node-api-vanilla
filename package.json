{
  "name": "vanilla-api",
  "version": "1.0.0",
  "description": "A boilerplate API project for nodejs, express and few more batteries",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dc-build": "docker-compose build",
    "dc-run": "docker-compose up",
    "dc-stop": "docker-compose down",
    "dockerbuild": "docker build -t vanilla-api .",
    "dockerrun": "docker run -p 8000:8000 vanilla-api",
    "ecrlogin": "aws ecr get-login-password | docker login --username AWS --password-stdin 344883437139.dkr.ecr.us-east-1.amazonaws.com",
    "tagimage": "docker tag node-api-vanilla:latest 344883437139.dkr.ecr.us-east-1.amazonaws.com/node-api-vanilla:latest",
    "pushimage": "docker push 344883437139.dkr.ecr.us-east-1.amazonaws.com/node-api-vanilla:latest",
    "verifyimage": "aws ecr list-images --repository-name node-api-vanilla --region us-east-1",
    "aws-deploy": "aws cloudformation create-stack --stack-name NodeApiVanilla --template-body file://aws/cf-template.yaml --capabilities CAPABILITY_IAM",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FullstackCodingGuy/node-api-vanilla.git"
  },
  "keywords": [
    "boilerplate",
    "nodejs",
    "expressjs"
  ],
  "author": "FullstackCodingGuy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FullstackCodingGuy/node-api-vanilla/issues"
  },
  "homepage": "https://github.com/FullstackCodingGuy/node-api-vanilla#readme",
  "dependencies": {
    "express": "^4.21.2",
    "serverless-http": "^3.2.0"
  }
}
